<script type="text/javascript">
$(document).ready(function(){
	
	mapBoard.init();
	$("#point> li, #point2> li").mouseover(function(){
		
		$(this).find('div').show();
		$(this).siblings().find('div').hide();
		$(this).parent()[0].id=="point2"?$(".point>li").find('div').hide():$(".point2>li").find('div').hide();
		
		mapBoard.mouseOverEvent(this.id);
	});
	
	$(".lv2_1, .lv2_2").mouseover(function(){
		
		$(this).find('div').show();
		$(this).siblings().find('div').hide();
		$(".point2>li").find('div').hide();
		
		mapBoard.mouseOverEvent(this.id.split("_")[0]);
	});
	
	$(".lv2_3").mouseover(function(){
		
		$(this).siblings().find('div').hide();
		$(".point>li").find('div').hide();
		$(".point_lv2").find('div').hide();
		$(this).find('div').show();
		
		mapBoard.mouseOverEvent(this.id.split("_")[0]);
	});
	
	$("#point> li, #point2> li, .lv2_1, .lv2_2, .lv2_3").mouseout(function(){
		mapBoard.mouseOutEvent();
	});
	
	$("#btn_type> a").click(function(){
		if(mapBoard.getTilteBtn()=='group1'){
			$(this).addClass("on");
			$(this).siblings().removeClass("on");
			mapBoard.onClickFilter(this.id);	
		}else{
			alert('현재 카테고리에서는 이용할 수 없습니다.');
		}
		
	});
	
	$("#playBtn").click(function(){
		mapBoard.onClickPlay(this);
	});
	
	$(".hanRiver").mouseover(function(){
		
		$(".img1").fadeIn(300);
		$(".img1").siblings().fadeOut(300);
	});
	
	$(".geumRiver").mouseover(function(){
		
		$(".img2").fadeIn(300);
		$(".img2").siblings().fadeOut(300);
	});
	
	$(".nakdongRiver").mouseover(function(){
		
		$(".img3").fadeIn(300);
		$(".img3").siblings().fadeOut(300);
	});
	
	$(".yeongsanRiver").mouseover(function(){
		
		$(".img4").fadeIn(300);
		$(".img4").siblings().fadeOut(300);
	});
	
	$(".hanRiver, .menu1").click(function(){
		
		$(".view1, .point_lv2 a").show();
		$(".view1").siblings().hide();
		mapBoard.onClickMap("hanRiver");
	});
	
	$(".geumRiver, .menu2").click(function(){
		
		$(".view2, .point_lv2 a").show();
		$(".view2").siblings().hide();	      
		mapBoard.onClickMap("geumRiver");
	});
	   
	$(".nakdongRiver, .menu3").click(function(){
		
		$(".view3, .point_lv2 a").show();
		$(".view3").siblings().hide();
		mapBoard.onClickMap("nakdongRiver");
	});
	   
	$(".yeongsanRiver, .menu4").click(function(){
		
		$(".view4, .point_lv2 a").show();
		$(".view4").siblings().hide();
		mapBoard.onClickMap("yeongsanRiver");
	});
	
	$(".hanRiver, .geumRiver, .nakdongRiver, .yeongsanRiver, .menu1, .menu2, .menu3, .menu4").click(function(){
		
		$(this).parents().parents().find('.menu').show();
		$(this).parents().parents().parents().find('#top_btn').show();
		$(this).parents().parents().parents().find('#legend').show();
	});
	
	$(".all").click(function(){
		
		$(".defualt, .point, .point2").show();
		$(this).parents().find(".view1, .view2, .view3, .view4").hide();
		$("#legendImg").attr("src","/weis_board/images/legend01.png");
		$("#legend").css("bottom","170px");
		
		mapBoard.onClickAllMapView();
	});
	
	$(".sea").mouseover(function(){
		
		$(this).parents().find('.img1, .img2, .img3, .img4').fadeOut(300);
	});
	
	$("#titlePreBtn, #titleNextBtn").click(function(){
		
		if(mapBoard.getGblFilter()!='all'){
			mapBoard.onClickTitleBtn(this.id);
			/* if(mapBoard.getTilteBtn()!='group1'){
				$("#point> li").hide();
			}else{
				$("#point> li").show();
			} */
		}else{
			alert('전체모드에서 이동할 수 없습니다.');
		}
		
	});
	
	$("#preBtn, #nextBtn").click(function(){
		mapBoard.onClickGridPreNextBtn(this.id);
	});
});
</script>
